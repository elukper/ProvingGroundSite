<?xml version="1.0" encoding="UTF-8"?>
<mapping>
<description>A mapping file for our Entries XML conversion</description>

<class name="hr.altima.xmlparser.parseddata.Person">
	<field name="name" type="string">
		<bind-xml name="entry" node="text" />
	</field>
	
	<field name="parentName" type="string">
		<bind-xml name="parentName" node="attribute" />
	</field>
</class>

<class name="hr.altima.xmlparser.parseddata.Entries">
	<field name="entries" type="hr.altima.xmlparser.parseddata.Person" collection="collection"/>
</class>

</mapping>